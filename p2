configuracion_2.Size = UDim2.new(0, 445, 0, 245)
configuracion_2.Visible = false

-- Scripts:

local function YRIEA_fake_script() -- Menu.LocalScript 
	local script = Instance.new('LocalScript', Menu)

	local toggleBtn = script.Parent
	local menuFrame = toggleBtn.Parent:WaitForChild("menuFrame")
	local UserInputService = game:GetService("UserInputService")
	local GuiService = game:GetService("GuiService")
	
	-- Screen adaptation function
	local function adaptToScreen()
		local screenSize = workspace.CurrentCamera.ViewportSize
		local isMobile = UserInputService.TouchEnabled
		
		if isMobile then
			-- Mobile adaptations
			toggleBtn.Size = UDim2.new(0, math.min(80, screenSize.X * 0.15), 0, math.min(50, screenSize.Y * 0.08))
			menuFrame.Size = UDim2.new(0.9, 0, 0.7, 0)
			
			-- Position button in corner for mobile
			toggleBtn.Position = UDim2.new(0, 15, 0, 15)
			
			-- Center menu initially
			menuFrame.Position = UDim2.new(0.5, -menuFrame.AbsoluteSize.X/2, 0.5, -menuFrame.AbsoluteSize.Y/2)
		else
			-- Desktop adaptations (keep original sizes)
			if toggleBtn.Size == UDim2.new(0, 116, 0, 49) then
				-- Keep original desktop size
			end
			if menuFrame.Size == UDim2.new(0, 561, 0, 293) then
				-- Keep original desktop size
			end
		end
		
		-- Ensure menu doesn't go off-screen
		task.wait(0.1) -- Wait for size to update
		if menuFrame.Position.X.Offset < 0 then
			menuFrame.Position = UDim2.new(0, 10, menuFrame.Position.Y.Scale, menuFrame.Position.Y.Offset)
		end
		if menuFrame.Position.X.Offset > screenSize.X - menuFrame.AbsoluteSize.X then
			menuFrame.Position = UDim2.new(0, screenSize.X - menuFrame.AbsoluteSize.X - 10, menuFrame.Position.Y.Scale, menuFrame.Position.Y.Offset)
		end
	end
	
	-- Toggle menu visibility (works for both mouse and touch)
	local function onToggle()
		menuFrame.Visible = not menuFrame.Visible
		
		-- Ensure menu is visible on screen when opened
		if menuFrame.Visible then
			local screenSize = workspace.CurrentCamera.ViewportSize
			if menuFrame.Position.X.Offset < 0 then
				menuFrame.Position = UDim2.new(0, 10, menuFrame.Position.Y.Scale, menuFrame.Position.Y.Offset)
			end
			if menuFrame.Position.X.Offset > screenSize.X - menuFrame.AbsoluteSize.X then
				menuFrame.Position = UDim2.new(0, screenSize.X - menuFrame.AbsoluteSize.X - 10, menuFrame.Position.Y.Scale, menuFrame.Position.Y.Offset)
			end
		end
	end
	
	toggleBtn.MouseButton1Click:Connect(onToggle)
	toggleBtn.TouchTap:Connect(onToggle)
	
	-- Improved draggable function for mobile
	local function makeDraggable(element)
		local dragging = false
		local dragStart = nil
		local startPos = nil
		local currentTouch = nil
		
		-- Handle both mouse and touch input start
		element.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or 
			   input.UserInputType == Enum.UserInputType.Touch then
				
				dragging = true
				dragStart = input.Position
				startPos = element.Position
				currentTouch = input
				
				-- Prevent default touch behavior for better dragging
				if input.UserInputType == Enum.UserInputType.Touch then
					GuiService:CaptureFocus(input)
				end
			end
		end)
		
		-- Handle input changes for both mouse and touch
		UserInputService.InputChanged:Connect(function(input)
			if not dragging then return end
			
			-- Only process the correct input (same touch or mouse movement)
			if input.UserInputType == Enum.UserInputType.MouseMovement or 
			   (input.UserInputType == Enum.UserInputType.Touch and input == currentTouch) then
				
				local delta = input.Position - dragStart
				local newPos = UDim2.new(
					startPos.X.Scale,
					startPos.X.Offset + delta.X,
					startPos.Y.Scale,
					startPos.Y.Offset + delta.Y
				)
				
				-- Keep element within screen bounds
				local screenSize = workspace.CurrentCamera.ViewportSize
				local elementSize = element.AbsoluteSize
				
				-- X bounds
				if newPos.X.Offset < 0 then
					newPos = UDim2.new(0, 0, newPos.Y.Scale, newPos.Y.Offset)
				elseif newPos.X.Offset > screenSize.X - elementSize.X then
					newPos = UDim2.new(0, screenSize.X - elementSize.X, newPos.Y.Scale, newPos.Y.Offset)
				end
				
				-- Y bounds
				if newPos.Y.Offset < 0 then
					newPos = UDim2.new(newPos.X.Scale, newPos.X.Offset, 0, 0)
				elseif newPos.Y.Offset > screenSize.Y - elementSize.Y then
					newPos = UDim2.new(newPos.X.Scale, newPos.X.Offset, 0, screenSize.Y - elementSize.Y)
				end
				
				element.Position = newPos
			end
		end)
		
		-- Handle input end for both mouse and touch
		UserInputService.InputEnded:Connect(function(input)
			if input == currentTouch and 
			   (input.UserInputType == Enum.UserInputType.MouseButton1 or 
			    input.UserInputType == Enum.UserInputType.Touch) then
				
				dragging = false
				currentTouch = nil
				
				-- Release focus
				if input.UserInputType == Enum.UserInputType.Touch then
					GuiService:ReleaseFocus()
				end
			end
		end)
	end
	
	-- Initial screen adaptation
	adaptToScreen()
	
	-- Re-adapt when screen size changes (device rotation, window resize)
	UserInputService:GetPropertyChangedSignal("ViewportSize"):Connect(adaptToScreen)
	
	-- Make both button and menu draggable
	makeDraggable(toggleBtn)
	makeDraggable(menuFrame)
end
coroutine.wrap(YRIEA_fake_script)()
local function AWZUQE_fake_script() -- opciones.FrameSwitchScript 
	local script = Instance.new('LocalScript', opciones)

	local screenGui = script.Parent.Parent
	local opcionesFrame = screenGui.opciones
	local frames = {screenGui.principal, screenGui.rentados, screenGui.premium, screenGui.gratis, screenGui.script, screenGui.configuracion}
	
	for _, frame in ipairs(frames) do
	    frame.Visible = false
	end
	
	opcionesFrame.principal.MouseButton1Click:Connect(function()
	    for _, frame in ipairs(frames) do frame.Visible = false end
	    screenGui.principal.Visible = true
	end)
	
	opcionesFrame.rentados.MouseButton1Click:Connect(function()
	    for _, frame in ipairs(frames) do frame.Visible = false end
	    screenGui.rentados.Visible = true
	end)
	
	opcionesFrame.premium.MouseButton1Click:Connect(function()
	    for _, frame in ipairs(frames) do frame.Visible = false end
	    screenGui.premium.Visible = true
	end)
	
	opcionesFrame.gratis.MouseButton1Click:Connect(function()
	    for _, frame in ipairs(frames) do frame.Visible = false end
	    screenGui.gratis.Visible = true
	end)
	
	opcionesFrame.script.MouseButton1Click:Connect(function()
	    for _, frame in ipairs(frames) do frame.Visible = false end
	    screenGui.script.Visible = true
	end)
	
	opcionesFrame.configuracion.MouseButton1Click:Connect(function()
	    for _, frame in ipairs(frames) do frame.Visible = false end
	    screenGui.configuracion.Visible = true
	end)
	
	screenGui.principal.Visible = true
end
coroutine.wrap(AWZUQE_fake_script)()
